{
    "name": "mip/symfony-reference",
    "description": "MIP Reference Implementation in Symfony",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle": "^6.4",
        "symfony/http-client": "^6.4",
        "symfony/twig-bundle": "^6.4",
        "symfony/yaml": "^6.4",
        "symfony/runtime": "^6.4",
        "symfony/dotenv": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "scripts": {
        "node1": "php -S localhost:4013 -t public",
        "node2": "php -S localhost:4014 -t public",
        "node3": "php -S localhost:4015 -t public"
    }
}
